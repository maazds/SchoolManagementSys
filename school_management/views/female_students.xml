<odoo>
    <data>
        ACTION
        <record model="ir.actions.act_window" id="female_student_management_action">
            <field name="name">Female Student DataBase</field>
            <field name="res_model">student.management</field>
            <field name="view_mode">tree,form</field>
            <field name="context">{'default_StudentGender':'1'}</field>
            <field name="domain">[('StudentGender','=','1')]</field>
        </record>
        <!--MENU-->
        <menuitem name="Female Students" id="female_student_menu" parent="student_main_menu"
                  action="female_student_management_action"/>
    </data>
</odoo>